{
    "name": "yxx/weekly-report",
    "description": "Generate weekly reports from git commits and GitHub issues, with email preview and confirmation workflow.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yxx",
            "email": "youyingxiang@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/mail": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Yxx\\WeeklyReport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yxx\\WeeklyReport\\WeeklyReportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
